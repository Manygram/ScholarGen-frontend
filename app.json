{
  "expo": {
    "name": "ScholarGen",
    "slug": "scholargen",
    "owner": "manygrame",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.timothyomhensele.scholargen",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.timothyomhensele.scholargen",
      "runtimeVersion": "1.0.0",
      "permissions": [
        "INTERNET"
      ]
    },
    "web": {
      "favicon": "./assets/splash-logo.png"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "1.9.25"
          }
        }
      ]
    ],
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "5f792f28-ed10-49b1-941c-55c62bce2dcf"
      }
    }
  }
}

